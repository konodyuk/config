<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<keys
   name="My custom shortcuts">
{% for item in inkscape.shortcuts %}
  <bind
    key="{{ item.key }}"
    action="{{ item.action }}"
{% if "modifiers" in item %}
    modifiers="{{ item.modifiers }}"
{% endif %}
    display="true"
  />
{% endfor %}

{% for item in inkscape.extensions %}
  <bind
    key="{{ item.key }}"
    action="com.autogenerated.{{ item.name }}"
{% if "modifiers" in item %}
    modifiers="{{ item.modifiers }}"
{% endif %}
    display="true"
  />
{% endfor %}
</keys>
