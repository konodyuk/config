<?xml version="1.0" encoding="UTF-8"?>
<inkscape-extension xmlns="http://www.inkscape.org/namespace/inkscape/extension">

<_name>{{ item.name }}</_name>

<id>com.autogenerated.{{ item.name }}</id>

<dependency type="executable" location="extensions">{{ item.name }}.py</dependency>

<effect implements-custom-gui="true">
    <object-type>all</object-type>
    <effects-menu>
        <submenu _name="submenu"/>
    </effects-menu>
</effect>
<script>
    <command reldir="extensions" interpreter="python">{{ item.name }}.py</command>
</script>
</inkscape-extension>
